{
  "name": "@arabasta/eslint-plugin-no-testing-library-without-cleanup",
  "version": "1.0.3",
  "description": "ESLint plugin that enforces the usage of the cleanup function when importing from Testing Library",
  "author": "Stoyan Kolev",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudNStoyan/arabasta"
  },
  "types": "./src/index.d.ts",
  "main": "./src/eslint-plugin-no-testing-library-without-cleanup.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/eslint-plugin-no-testing-library-without-cleanup.js"
    }
  },
  "scripts": {
    "test": "vitest --globals --dir ./src/rules"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "testing-library"
  ]
}
